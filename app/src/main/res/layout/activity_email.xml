<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"

    android:padding="25dp"

    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        android:id="@+id/textView"
        android:capitalize="characters"
        android:clickable="true"
        android:editable="true"
        android:textAlignment="center"
        android:autoText="false"
        android:textColor="#ffffffff"
        android:textSize="20dp"
        android:layout_marginTop="66dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Version 1.0.0"
        android:id="@+id/textView2"
        android:gravity="right"
        android:textStyle="italic"
        android:textColor="#ffffffff"
        android:layout_below="@+id/textView"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"

        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="34dp"
        android:focusable="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Application Developers:"
        android:id="@+id/textView3"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="29dp"
        android:textColor="#ffffffff"
        android:textStyle="bold|italic" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Raaghul.R"
        android:id="@+id/textView4"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="15dp"
        android:textColor="#ffffffff"
        android:textStyle="italic"
        android:textSize="20sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="B.E - CSE"
        android:id="@+id/textView5"
        android:textColor="#ffffffff"
        android:textStyle="italic"
        android:textSize="20sp"
        android:layout_alignTop="@+id/textView4"
        android:layout_toRightOf="@+id/textView3"
        android:layout_toEndOf="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="FeedBack:"
        android:id="@+id/textView6"
        android:layout_marginTop="69dp"
        android:textColor="#ffffffff"
        android:textStyle="bold|italic"
        android:padding="5dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="raaghulrdev@gmail.com"
        android:id="@+id/editText"
        android:textColor="#ffffffff"
        android:textStyle="bold|italic"
        android:textIsSelectable="true"
        android:clickable="true"
        android:onClick="emailme"
        android:inputType="none"
        android:background="#4b000000"
        android:textSize="20sp"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Click Email Id below to give your FeedBack"
        android:id="@+id/textView7"
        android:textColor="#ffffffff"
        android:layout_below="@+id/textView6"
        android:layout_alignStart="@+id/textView11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Sri Raam.M"
        android:id="@+id/textView8"

        android:textColor="#ffffffff"
        android:textStyle="italic"
        android:textSize="20sp"
        android:layout_below="@+id/textView4"
        android:layout_alignStart="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="B.E - CSE"
        android:id="@+id/textView9"
        android:textColor="#ffffffff"
        android:textStyle="italic"
        android:textSize="20sp"
        android:layout_alignTop="@+id/textView8"
        android:layout_toEndOf="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Suki.R"
        android:id="@+id/textView10"

        android:textColor="#ffffffff"
        android:textStyle="italic"
        android:textSize="20sp"
        android:layout_below="@+id/textView8"
        android:layout_alignStart="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Snehaa.S"
        android:id="@+id/textView11"
        android:textColor="#ffffffff"
        android:textStyle="italic"
        android:textSize="20sp"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="B.E - CSE"
        android:id="@+id/textView12"
        android:textColor="#ffffffff"
        android:textStyle="italic"
        android:textSize="20sp"
        android:layout_below="@+id/textView9"
        android:layout_toEndOf="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="B.E - CSE"
        android:id="@+id/textView13"
        android:textColor="#ffffffff"
        android:textStyle="italic"
        android:textSize="20sp"
        android:layout_below="@+id/textView12"
        android:layout_toEndOf="@+id/textView3" />

</RelativeLayout>


